[user]
    name = Adam Mansfield
    email = me@adammansfield.com
[core]
    autocrlf = input
    editor = vim
    excludesfile = ~/.gitglobalignore
    filemode = false
[color]
    ui = true
[help]
	  autocorrect = 2
[push]
	  default = current
[alias]
    accept-ours = "!f() { [ -z \"$@\" ] && set - '.'; git checkout --ours -- \"$@\"; git add -u -- \"$@\"; }; f"
    accept-theirs = "!f() { [ -z \"$@\" ] && set - '.'; git checkout --theirs -- \"$@\"; git add -u -- \"$@\"; }; f"
