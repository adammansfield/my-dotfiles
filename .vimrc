set autoindent
set backspace=indent,eol,start  " Allow backspacing in insert mode
set expandtab       " Spaces instead of tabs
set nocompatible
set ruler           " Show cursor position
set shiftwidth=4    " Four(4) spaces for indenting
set tabstop=4       " Four(4) stops
set undolevels=100

" Colors
syntax on
"colorscheme darkblue
colorscheme murphy
